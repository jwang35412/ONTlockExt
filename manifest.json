{
    "name": "ONTlockWallet",
    "author": "Jawax",
    "description": "Decentralized password manager hosted on the Ontology blockchain",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
      "default_icon": "img/icon.png",
      "default_popup": "index.html"
    },
    "permissions": [
      "tabs",
      "activeTab",
      "clipboardWrite"
    ],
    "web_accessible_resources": ["img/OntLogo.png"]
  }
